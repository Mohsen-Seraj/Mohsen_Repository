@model IEnumerable<datalayer.page>


<section class="last-news-side border-radius">
    <header><i class="icons icon-star"></i> <h3> پربازدیدترین اخبار </h3></header>
    <ul>

        @foreach (var item in Model)
        {

            <li>
                <img class="border-radius" src="~/images/@item.imagename">
                <h2> <a href="/news/@item.pageid">@item.title</a></h2>
                <span>انتشار : @item.createdate.toshamsi()</span>
            </li>




        }

    </ul>
</section>